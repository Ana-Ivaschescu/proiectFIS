<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AddPlayerController">
   <children>
      <Text layoutX="157.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Text layoutX="157.0" layoutY="171.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Position">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Text layoutX="157.0" layoutY="219.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Text layoutX="164.0" layoutY="48.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter the player's data">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <TextField fx:id="playerNameField" layoutX="300.0" layoutY="101.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="playerPositionField" layoutX="300.0" layoutY="151.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="playerDescriptionField" alignment="TOP_LEFT" layoutX="300.0" layoutY="199.0" prefHeight="99.0" prefWidth="175.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button layoutX="475.0" layoutY="334.0" mnemonicParsing="false" onAction="#saveButtonPushed" text="Save">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="82.0" layoutY="334.0" mnemonicParsing="false" onAction="#backButtonPushed" text="Back">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
